<div id="courseListModal" class="modal fade">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Liste gewählter Veranstaltungen</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body">
                <p>Hier findest du alle deine gewählten Veranstaltungen in einer Listenansicht</p>
                <paula-course-list course-list="vm.sync.CourseList.SelectedCourses"
                                   add-course="addCourse"
                                   remove-course="removeCourse"/>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div>